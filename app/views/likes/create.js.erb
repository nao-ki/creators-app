<% if @sentence %>

 $("#like-<%= @sentence.id %>").html("<%= j(render partial: 'sentence_like', locals: { sentence: @sentence }) %>");

<% elsif @video %>

 $("#like-<%= @video.id %>").html("<%= j(render partial: 'video_like', locals: { video: @video }) %>");

<% elsif @sound %>

 $("#like-<%= @sound.id %>").html("<%= j(render partial: 'sound_like', locals: { sound: @sound }) %>");

<% else %>

 $("#like-<%= @image.id %>").html("<%= j(render partial: 'image_like', locals: { image: @image }) %>");

<% end%>
